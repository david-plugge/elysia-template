{
	"name": "elysia-template",
	"version": "1.0.0",
	"type": "module",
	"module": "src/index.js",
	"scripts": {
		"dev": "bun run --watch src/index.ts",
		"build": "bun build --compile --minify --sourcemap --target=bun-linux-x64 --bytecode ./src/index.ts --outfile build/app",
		"format": "biome format --write .",
		"lint": "biome lint --error-on-warnings .",
		"type-check": "tsc --noEmit",
		"lint:write": "biome lint --write ."
	},
	"dependencies": {
		"@bogeychan/elysia-logger": "^0.1.4",
		"@elysiajs/cors": "^1.1.1",
		"@elysiajs/opentelemetry": "^1.1.6",
		"@elysiajs/server-timing": "^1.1.0",
		"@elysiajs/swagger": "^1.1.5",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@oslojs/oauth2": "^0.5.0",
		"@sinclair/typebox": "0.33.16",
		"elysia": "^1.1.22",
		"ioredis": "^5.4.1",
		"postgres": "^3.4.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.3",
		"bun-types": "1.1.30",
		"typescript": "^5.6.3"
	},
	"overrides": {
		"@sinclair/typebox": "0.33.16"
	},
	"packageManager": "bun@1.1.30"
}
